@prefix align: <http://knowledgeweb.semanticweb.org/heterogeneity/alignment#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://kb.virtualtreasury.ie/alignment> a align:Alignment ;
    align:map [ a align:Cell ;
            align:entity1 <http://erlangen-crm.org/current/E21_Person> ;
            align:entity2 foaf:Person ;
            align:measure "1.0"^^xsd:float ;
            align:relation "=" ] ;
    align:onto1 <https://kb.virtualtreasury.ie/ontology> ;
    align:onto2 foaf: .



